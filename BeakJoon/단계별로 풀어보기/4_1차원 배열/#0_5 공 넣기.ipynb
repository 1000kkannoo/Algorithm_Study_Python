    "limit, count = map(int,input().split())\n",
    "\n",
    "sumList = [0] * limit\n",
    "\n",
    "for i in range(count):\n",
    "    minCount, maxCount, ball = map(int,input().split())\n",
    "    for j in range(minCount - 1, maxCount):\n",
    "        sumList[j] = ball\n",
    "print(*sumList)"
