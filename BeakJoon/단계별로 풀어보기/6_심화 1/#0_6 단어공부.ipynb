   "source": [
    "word = input().upper()\n",
    "wordList = list(set(word))\n",
    "cntList = []\n",
    "\n",
    "for value in wordList:\n",
    "    cnt = word.count(value)\n",
    "    cntList.append(cnt)\n",
    "\n",
    "if cntList.count(max(cntList)) >= 2 :\n",
    "    print(\"?\")\n",
    "else:\n",
    "    print(wordList[cntList.index(max(cntList))])"
   ]
  }
