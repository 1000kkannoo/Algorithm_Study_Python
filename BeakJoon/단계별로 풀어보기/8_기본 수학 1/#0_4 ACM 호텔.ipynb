    "import math\n",
    "\n",
    "count = int(input())\n",
    "\n",
    "for _ in range(count):\n",
    "    \n",
    "    h, w, n = map(int,input().split())\n",
    "    a = n - ((n // h) * h)\n",
    "    if(a == 0):\n",
    "        a = h\n",
    "    \n",
    "    b = math.ceil((n / h))\n",
    "    \n",
    "    if b < 10:\n",
    "        print(f'{a}0{b}')\n",
    "    else:\n",
    "        print(f'{a}{b}')"
   ]
  }
