    "T = int(input())\n",
    "\n",
    "for _ in range(T):\n",
    "    k = int(input())\n",
    "    n = int(input())\n",
    "    p = [ i for i in range(1, n+1)]\n",
    "\n",
    "    for __ in range(k):\n",
    "        for j in range(1,n):\n",
    "            p[j] += p[j-1]\n",
    "\n",
    "    print(p[-1])"
   ]
