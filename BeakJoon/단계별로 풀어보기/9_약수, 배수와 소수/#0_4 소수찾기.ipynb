   "source": [
    "n = int(input())\n",
    "num = list(map(int, input().split()))\n",
    "cnt = 0\n",
    "\n",
    "for i in range(n):\n",
    "    save = []\n",
    "    for j in range(1, num[i] + 1):\n",
    "        if num[i] % j == 0:\n",
    "            save.append(j)\n",
    "    if len(save) == 2:\n",
    "        cnt += 1\n",
    "print(cnt)"
   ]
  }
