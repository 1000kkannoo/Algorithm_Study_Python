   "source": [
    "count = int(input())\n",
    "sumList = []\n",
    "word = \"\"\n",
    "\n",
    "for i in range(count):\n",
    "    a,b = map(str, input().split())\n",
    "    \n",
    "    for value in b:\n",
    "        word += value * int(a)\n",
    "        \n",
    "    sumList.append(word)\n",
    "    word = \"\"\n",
    "    \n",
    "print(*sumList, sep = \"\\n\")\n",
    "            "
   ]
  }
