    "h,m = map(int,input().split())\n",
    "time = int(input())\n",
    "\n",
    "if(m + time >= 60):\n",
    "    sum = m + time\n",
    "    time = sum // 60\n",
    "    m = sum % 60\n",
    "    \n",
    "    h += time\n",
    "    \n",
    "    if(h >= 24):\n",
    "        h -= 24\n",
    "else:\n",
    "    m += time\n",
    "\n",
    "print(h,m)"
   ]
  }
